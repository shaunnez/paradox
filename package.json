    "lint": "./node_modules/.bin/eslint js",
    "deploy": "gh-pages-deploy"
  },
  "author": "Shaun Nesbitt",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/shaunnez/paradox.git"
  },
  "dependencies": {},
  "devDependencies": {
    "animate.css": "git+https://github.com/daneden/animate.css.git",
    "babel-core": "latest",
    "babel-eslint": "latest",
    "babel-loader": "latest",
    "babel-preset-es2015": "latest",
    "bootstrap": "latest",
    "css-loader": "latest",
    "eslint": "latest",
    "extract-text-webpack-plugin": "latest",
    "file-loader": "latest",
    "font-awesome": "latest",
    "gh-pages": "^0.11.0",
    "gh-pages-deploy": "^0.4.2",
    "git-directory-deploy": "^1.5.1",
    "jquery": "latest",
    "jquery.scrollto": "latest",
    "node-sass": "latest",
    "postcss-loader": "latest",
    "sass-loader": "latest",
    "style-loader": "latest",
    "url-loader": "latest",
    "waypoints": "latest",
    "webpack": "latest",
    "webpack-dev-server": "latest"
  },
  "gh-pages-deploy": {
    "staticpath": "build",
    "cname": "https://shaunnez.github.io/paradox/",
    "prep": [
